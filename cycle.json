{
  "version": "1.0",
  "containers": {
    "demo": {
      "name": "changed demo image",
      "image": {
        "name": "simplews",
        "origin": {
          "type": "docker-file",
          "details": {
            "dir": "/",
            "path": "/Dockerfile"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "simplews",
          "ports": ["80:80"]
        },
        "deploy": {
          "instances": 1
        }
      }
    },
    "reach": {
      "name": "updated reach network settings",
      "image": {
        "name": "simplews-external",
        "origin": {
          "type": "docker-file",
          "details": {
            "url": "https://github.com/aubuchcl/build-dir-two.git",
            "dir": "/",
            "path": "/Dockerfile"
          }
        }
      },
      "config": {
        "network": {
          "public": "disable",
          "hostname": "simplews",
          "ports": ["80:80"]
        },
        "deploy": {
          "instances": 1
        }
      }
    },
    "stacktest": {
      "name": "stacktest",
      "image": {
        "name": "stacktest",
        "origin": {
          "type": "docker-hub",
          "details": {
            "target": "cycleplatform/debug:no-volume"
          }
        }
      },
      "config": {
        "network": {
          "public": "disable",
          "hostname": "stacktest"

        },
        "deploy": {
          "instances": 1
        }
      }
    }
  }
}
